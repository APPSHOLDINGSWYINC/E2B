# Environment variables for production environment
# Copy this file to .env.production and fill in the values

# E2B API Configuration
E2B_API_KEY=your_production_api_key_here

# Deployment Configuration
ENVIRONMENT=production
DEPLOY_URL=https://e2b.dev

# Node.js Configuration
NODE_ENV=production

# Database Configuration (if applicable)
# DATABASE_URL=your_production_database_url

# Redis Configuration (if applicable)
# REDIS_URL=your_production_redis_url

# Cloud Provider Configuration (if applicable)
# GCP_PROJECT_ID=e2b-production
# AWS_REGION=us-east-1

# Monitoring and Logging
# SENTRY_DSN=your_sentry_dsn
# LOG_LEVEL=warn

# Feature Flags
# ENABLE_FEATURE_X=true
