# Environment variables for production environment
# Copy this file to .env.production and fill in the values
# Set these in Vercel Dashboard: https://vercel.com/dashboard/settings/environment-variables

# ============================================
# Required Variables
# ============================================

# E2B API Configuration
E2B_API_KEY=your_production_api_key_here

# Next.js Public Variables (accessible in browser)
NEXT_PUBLIC_API_URL=https://your-app.vercel.app
NEXT_PUBLIC_E2B_API_URL=https://api.e2b.dev

# ============================================
# Optional API Keys
# ============================================

# AI Service API Keys (if using)
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENAI_API_KEY=your_openai_key_here
GOOGLE_API_KEY=your_google_key_here

# ============================================
# Deployment Configuration
# ============================================

ENVIRONMENT=production
DEPLOY_URL=https://e2b.dev
PRODUCTION_URL=https://e2b.dev

# Node.js Configuration
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# ============================================
# Vercel Configuration (auto-populated)
# ============================================

VERCEL=1
VERCEL_ENV=production

# ============================================
# Database Configuration (if applicable)
# ============================================

# DATABASE_URL=your_production_database_url
# REDIS_URL=your_production_redis_url

# ============================================
# Cloud Provider Configuration (if applicable)
# ============================================

# GCP_PROJECT_ID=e2b-production
# AWS_REGION=us-east-1

# ============================================
# Monitoring and Logging
# ============================================

# SENTRY_DSN=your_sentry_dsn
# LOG_LEVEL=warn

# ============================================
# GitHub Configuration (for deployments)
# ============================================

# GITHUB_TOKEN=your_github_token_here
# GITHUB_OWNER=APPSHOLDINGSWYINC
# GITHUB_REPO=E2B

# ============================================
# Feature Flags
# ============================================

# ENABLE_FEATURE_X=true

