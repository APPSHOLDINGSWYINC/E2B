# Environment variables for staging environment
# Copy this file to .env.staging and fill in the values

# E2B API Configuration
E2B_API_KEY=your_staging_api_key_here

# Deployment Configuration
ENVIRONMENT=staging
DEPLOY_URL=https://staging.e2b.dev

# Node.js Configuration
NODE_ENV=staging

# Database Configuration (if applicable)
# DATABASE_URL=your_staging_database_url

# Redis Configuration (if applicable)
# REDIS_URL=your_staging_redis_url

# Cloud Provider Configuration (if applicable)
# GCP_PROJECT_ID=e2b-staging
# AWS_REGION=us-east-1

# Monitoring and Logging
# SENTRY_DSN=your_sentry_dsn
# LOG_LEVEL=info

# Feature Flags
# ENABLE_FEATURE_X=true
